<%- include('partials/header',{head:seo.attributes.SEO,link:link,baseURL:baseURL,pageURL:pageURL}) %> <%- include('partials/nav') %>
    <header
      class="md:snap-normal flex md:block md:snap-center 2xl:max-w-[1236px] xl:mx-w-[1080px] md:px-0 lg:max-w-[824px] md:max-w-[768px] px-5 sm:max-w-[640px] max-w-full mx-auto h-screen md:pt-[180px]">
      <img src="/public/img/MASSIVE_WORKS.svg" alt="Massive Work" class="w-full" />
    </header>
    <description-a
      class="md:snap-normal 2xl:max-w-[1236px] xl:mx-w-[1080px] lg:max-w-[824px] md:max-w-[768px] sm:max-w-[640px] max-w-full mx-auto h-screen md:snap-center md:px-0 px-5 content-center grid md:grid-cols-2 grid-cols-1">
      <p class="font-instrument text-white mb-[34px] md:mb-0 text-base md:text-2xl max-w-[80%]">
        <%= data.attributes.HomePara1  %>
      </p>
    </description-a>
    <established-a class="text-center scrollReveal md:snap-normal md:snap-center bg-white block">
      <div class="sticky top-0 h-screen bg-opacity-0 bg-white">
        <div class="relative h-full md:px-16 px-5 flex flex-col justify-center">
          <div class="borderTop absolute origin-top top-0 left-0 w-full h-1/2 bg-black overflow-hidden">
            <p 
              class="z-[1] support absolute bottom-16 md:bottom-64 w-full text-center text-base md:text-2xl mb-0 italic font-instrument text-white">
              Estd.
            </p>
            <p
              class="topHead z-[1] font-thunder absolute -bottom-2 md:-bottom-10 w-full text-center leading-[0px] text-white text-[125px] md:text-[31.25rem]">
              2015
            </p>
          </div>
          <div class="borderBottom absolute origin-bottom bottom-0 left-0 w-full h-1/2 bg-black overflow-hidden">
            <p 
              class="z-[1] bottomHead absolute top-2 md:top-10 w-full h-auto text-center font-thunder leading-[1px] text-white text-[125px] md:text-[31.25rem]">
              2015
            </p>
          </div>
        </div>
      </div>
      <div class="bg-white text-left">
        <quotes-white
          class="md:px-0 2xl:max-w-[1236px] xl:mx-w-[1080px] lg:max-w-[824px] md:max-w-[768px] sm:max-w-[640px] max-w-full mx-auto md:snap-center md:snap-normal text-4xl md:text-[6rem] lg:text-[10rem] leading-[85%] px-5 bg-white md:justify-between flex-col md:flex-row text-black min-h-screen flex py-40 md:py-80">
          <div class="flex flex-col justify-center order-2 md:order-1 flex-grow md:flex-grow-0">
            <p class="font-instrument text-3xl md:text-[4rem] leading-[85%] lg:text-[8rem] mb-1 md:mb-[14px]">
              We
            </p>
            <p class="font-instrument italic mb-1 md:mb-[14px]">Reinvigorate</p>
            <p class="font-instrument">Design.</p>
          </div>
          <div class="flex-shrink-0 text-right order-1 md:order-2">
            <p class="font-instrument text-base md:text-2xl">what do we do?</p>
          </div>
        </quotes-white>
      </div>
      <div class="bg-black text-left">
        <quotes-black
        class="md:px-0 2xl:max-w-[1236px] xl:mx-w-[1080px] lg:max-w-[824px] md:max-w-[768px] sm:max-w-[640px] max-w-full mx-auto md:snap-normal md:snap-center text-4xl md:text-[6rem] lg:text-[10rem] leading-[85%] px-5 bg-black md:justify-between flex-col md:flex-row text-white min-h-screen flex py-40 md:py-80">
        <div class="flex flex-col justify-center order-2 md:order-1 flex-grow md:flex-grow-0">
          <p class="font-instrument text-4xl md:text-[6rem] leading-[85%] lg:text-[10rem] mb-1 md:mb-[14px]">
            With
          </p>
          <p class="font-instrument italic mb-1 md:mb-[14px]">Robust</p>
          <p class="font-instrument italic">Elegance.</p>
        </div>
        <div class="flex-shrink-0 text-right order-1 md:order-2">
          <p class="font-instrument text-base md:text-2xl">how do we do it?</p>
        </div>
      </quotes-black>
      </div> 
    </established-a>
    
    <work-title class="text-center scrollReveal md:snap-normal md:snap-center bg-black block">
      <div class="sticky top-0 h-screen bg-opacity-0 bg-black">
        <div class="relative h-full md:px-16 px-5 flex flex-col justify-center text-black">
          <div class="borderTop absolute pt-20 origin-top top-0 left-0 w-full h-1/2 bg-white overflow-hidden">
            <div class="px-5 md:px-0 2xl:max-w-[1236px] xl:mx-w-[1080px] lg:max-w-[824px] md:max-w-[768px] sm:max-w-[640px] max-w-full flex flex-grow flex-wrap justify-between font-instrument md:mb-[14rem] mb-[12rem] mx-auto">
              <h3 class="support md:text-7xl md:block hidden leading-[100%] text-base">
                Take a look at our
              </h3>
              <p class="support text-right md:text-2xl text-base md:w-auto w-full">
                don't belive us?
              </p>
            </div>
            <h3 class="support md:text-[6.875rem] font-instrument text-center md:hidden leading-[100%] mb-5 text-base">
              Take a look at our
            </h3>
            <p
              class="z-[1] topHead font-thunder absolute -bottom-2 md:-bottom-10 w-full text-center leading-[0px] text-black text-[125px] md:text-[31.25rem]">
              WORK
            </p>
          </div>
          <div class="borderBottom absolute origin-bottom bottom-0 left-0 w-full h-1/2 bg-white overflow-hidden">
            <p
              class="z-[1] bottomHead absolute top-2 md:top-10 w-full h-auto text-center font-thunder leading-[1px] text-black text-[125px] md:text-[31.25rem]">
              WORK
            </p>
          </div>
        </div>
      </div>
      <div class="bg-black">
        <div class="bg-black py-10 pt-40  md:pt-80">
          <% data.attributes.works.data.forEach((workData , key)=>{ %>
          <work-single class=" block px-5  pb-6 md:px-0">
            <div class="overflow-hidden h-[9rem] md:h-[26rem] relative">
              <a href="/work/<%= works.attributes.works.data[key].attributes.slug %>"
                class="h-[9rem] bg-black md:h-[26rem] transition-all duration-1000 group hover:scale-125 flex justify-center bg-opacity-20"
                style="
                background: url('<%=link%><%= works.attributes.works.data[key].attributes.DisplayHomeBanner.data.attributes.url %>');
                background-repeat: no-repeat;
                background-position: 50% 50%;
                background-size: cover;
              " role="img" aria-label="<%= works.attributes.works.data[key].attributes.DisplayHomeBanner.data.attributes.alternativeText %>">
              </a>
            </div>
            <div class="flex justify-between font-instrument text-white md:px-24 md:text-lg py-2.5">
              <div><%= data.attributes.works.data[key].attributes.ClientName %></div>
              <div class="italic"><%= data.attributes.works.data[key].attributes.TypeOfProject %></div>
              <div><%= data.attributes.works.data[key].attributes.YearOfProject %></div>
            </div>
          </work-single>
          <% }) %>
        </div>
      </div>
      
    </work-title>
    
    <div class="bg-white py-40 md:px-0 px-5">
      <a id="viewmore" href="/work"
        class="md:px-0 2xl:max-w-[1236px] xl:mx-w-[1080px] lg:max-w-[824px] md:max-w-[768px] sm:max-w-[640px] max-w-full mx-auto md:snap-normal md:snap-center flex group  justify-center">
        <div id="viewChild1"
          class="h-9 md:h-40 group-hover:-mr-5 flex md:mt-[0.15rem] place-content-center transition-all duration-1000">
          <img class="block w-full" src="/public/img/VIEW.svg">
        </div>
        <div id="viewChild2"
          class="w-0 flex md:mt-[0.15rem] place-content-center h-9 md:h-40 bg-black transition-all duration-1000 group-hover:px-6 group-hover:w-40">
          <img class="block" src="/public/img/arrow.svg">
        </div>
        <div id="viewChild3"
          class="h-9 md:h-[10.27rem] group-hover:-ml-5 flex place-content-center transition-all duration-1000"><img
            class="block" src="/public/img/More.svg"></div>
      </a>
    </div>

    <%- include('partials/transation',{rescode:rescode,resmessage:resmessage}) %>
    <%- include('partials/footer')%>